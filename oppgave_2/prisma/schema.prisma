generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Athlete {
  id            String            @id @default(cuid())
  userId        String            @unique
  gender        String
  sportType     String
  //skal v√¶re en array av strings
  meta          String?
}
